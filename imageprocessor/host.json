{
    "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "default": "Warning",
      "Host": "Warning",
      "Function": "Information",
      "Host.Aggregator": "Information"
    },
    "extensions": {
      "queues": {
        "batchSize": 1,
        "maxPollingInterval": "00:00:30",
        "visibilityTimeout": "00:00:30",
        "maxDequeueCount": 5,
        "newBatchThreshold": 0
      }
    }
  }
}